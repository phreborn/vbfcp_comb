<!DOCTYPE Orgnization  SYSTEM 'Orgnization.dtd'>
<Orgnization InFile="workspace/fcnc/decorated/hgg_decorated.root" OutFile="workspace/fcnc/param/hgg_param.root"
	     ModelName="FCNC_mu"
	     POINames="mu_Hc, mu_Hu">
  <Item Name="mu_Hc[1,-50,50]"/>
  <Item Name="mu_Hu[1,-50,50]"/>
  <Item Name="mu_BR[1,-50,50]"/>
  <Item Name="onepercent[0.01]"/>
  <Item Name="zero[0]"/>
  <Item Name="one[1]"/>
  <Item Name="kxs_had_updated[0.068]"/>
  <Item Name="kxs_lep_updated[0.064]"/>
  
  <Item Name="expr::mu_lep_composite('(@0+0.94*@1)*@2*(1-@2)', mu_Hc, mu_Hu, onepercent)"/>
  <Item Name="expr::mu_had_composite('(@0+1.01*@1)*@2*(1-@2)', mu_Hc, mu_Hu, onepercent)"/>

  <!-- Introducing kappa values -->
  <!-- H->gamgam -->
  <Item Name="kappa_value_BR_param_alphaS_gamgam[1.012]"/>
  <Item Name="kappa_value_BR_param_mB_gamgam[0.981]"/>
  <Item Name="kappa_value_BR_HiggsDecayWidthTHU_hqq_gamgam[0.988]"/>
  <Item Name="kappa_value_BR_HiggsDecayWidthTHU_hgg_gamgam[1.010]"/>

  <!-- Introducing nuisance parameter names -->
  <Item Name="BR_param_alphaS[0,-5,5]"/>
  <Item Name="BR_param_mB[0,-5,5]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hqq[0,-5,5]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hgg[0,-5,5]"/>

  <!-- Introducing global observables -->
  <Item Name="BR_param_alphaS_In[0]"/>
  <Item Name="BR_param_mB_In[0]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hqq_In[0]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hgg_In[0]"/>

  <!-- Introducing constraint terms -->
  <Item Name="RooGaussian::BR_param_alphaS_Pdf(BR_param_alphaS,BR_param_alphaS_In,1)"/>
  <Item Name="RooGaussian::BR_param_mB_Pdf(BR_param_mB,BR_param_mB_In,1)"/>
  <Item Name="RooGaussian::BR_HiggsDecayWidthTHU_hqq_Pdf(BR_HiggsDecayWidthTHU_hqq,BR_HiggsDecayWidthTHU_hqq_In,1)"/>
  <Item Name="RooGaussian::BR_HiggsDecayWidthTHU_hgg_Pdf(BR_HiggsDecayWidthTHU_hgg,BR_HiggsDecayWidthTHU_hgg_In,1)"/>

  <!-- Introducing response terms -->
  <!-- H->gamgam -->
  <Item Name="prod::expected_BR_param_alphaS_gamgam(RooExponential::expTerm_BR_param_alphaS_gamgam(BR_param_alphaS, expr::log_kappa_value_BR_param_alphaS_gamgam('log(@0)', kappa_value_BR_param_alphaS_gamgam)),nominal_BR_param_alphaS_gamgam[1])"/>
  <Item Name="prod::expected_BR_param_mB_gamgam(RooExponential::expTerm_BR_param_mB_gamgam(BR_param_mB, expr::log_kappa_value_BR_param_mB_gamgam('log(@0)', kappa_value_BR_param_mB_gamgam)),nominal_BR_param_mB_gamgam[1])"/>
  <Item Name="prod::expected_BR_HiggsDecayWidthTHU_hqq_gamgam(RooExponential::expTerm_BR_HiggsDecayWidthTHU_hqq_gamgam(BR_HiggsDecayWidthTHU_hqq, expr::log_kappa_value_BR_HiggsDecayWidthTHU_hqq_gamgam('log(@0)', kappa_value_BR_HiggsDecayWidthTHU_hqq_gamgam)),nominal_BR_HiggsDecayWidthTHU_hqq_gamgam[1])"/>
  <Item Name="prod::expected_BR_HiggsDecayWidthTHU_hgg_gamgam(RooExponential::expTerm_BR_HiggsDecayWidthTHU_hgg_gamgam(BR_HiggsDecayWidthTHU_hgg, expr::log_kappa_value_BR_HiggsDecayWidthTHU_hgg_gamgam('log(@0)', kappa_value_BR_HiggsDecayWidthTHU_hgg_gamgam)),nominal_BR_HiggsDecayWidthTHU_hgg_gamgam[1])"/>
  <Item Name="prod:expected_BR_gamgam_new(expected_BR_param_alphaS_gamgam,expected_BR_param_mB_gamgam,expected_BR_HiggsDecayWidthTHU_hqq_gamgam,expected_BR_HiggsDecayWidthTHU_hgg_gamgam,mu_BR_gamgam[1])"/>

  <Item Name="prod:multiplier_lep(expected_BR_gamgam_new, mu_lep_composite)"/>
  <Item Name="prod:multiplier_had(expected_BR_gamgam_new, mu_had_composite)"/>
  
  <Item Name= "EDIT::NEWPDF(OLDPDF,
	       b1b_had=multiplier_had,
	       b1b_lep=multiplier_lep,
	       b1b_lepCR=multiplier_lep,
	       constrbr_had=one,
	       nuibr=zero,
	       kxs_had=kxs_had_updated,
	       kxs_lep=kxs_lep_updated,
       	       )" />
</Orgnization>
