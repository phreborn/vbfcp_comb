<!DOCTYPE Combination  SYSTEM 'Combination.dtd'>
<Combination>
  <Channel Name="combined" IsCombined="true" Mass="#MASS#">
    <File Name="./combined.root"/>
    <Workspace Name="combWS"/>
    <ModelConfig  Name="ModelConfig"/>
    <ModelData Name="combData"/>
    <ModelPOI Name="xs[0~100],dxs[-100~100],mX[750~750],wX[45~45]"/>
  </Channel>
  <Channel Name="high_mass_diphoton_13TeV">
    <File Name="#INPUT13TeV#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="xs,dxs,mX,wX"/>
    <ModelData Name="combData"/>
    <RenameMap>
      &13TeV_npmap.xml;
    </RenameMap>
  </Channel>
  <Channel Name="high_mass_diphoton_8TeV">
    <File Name="#INPUT8TeV#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="xs,dummy,mX,wX"/>
    <ModelData Name="combData"/>
    <RenameMap>
      &8TeV_npmap.xml;
    </RenameMap>
  </Channel>
</Combination>
