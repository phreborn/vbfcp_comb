    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="xs_new,dxs,mG,GkM"/>
    <ModelData Name="combDatabinned"/>
    <RenameMap>
<Syst OldName =     "constr__ATLAS_lumi_2015(ATLAS_lumi_2015,RNDM__ATLAS_lumi_2015)"       NewName =       "ATLAS_lumi_2015" />
<Syst OldName =     "constr__pdf_gg(pdf_gg,RNDM__pdf_gg)"       NewName =       "pdf_gg" />
