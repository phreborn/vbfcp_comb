<!DOCTYPE Combination  SYSTEM 'Combination.dtd'>
<!-- Range 1: from 700 GeV (included) to 1000 GeV (not included). lvqq+llqq  -->
<Combination>
  <Channel Name="combined" IsCombined="true" Mass="#MASS#">
    <File Name="./combined.root"/>
    <Workspace Name="combWS"/>
    <ModelConfig  Name="ModelConfig"/>
    <ModelData Name="combData"/>
    <ModelPOI Name="xs[0~100],dxs[-100~100],mG[760~760],GkM[0.2~0.2]"/>
  </Channel>
  <Channel Name="high_mass_diphoton_13TeV">
    <File Name="#INPUT13TeV#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="xs_new,dxs,mG,GkM"/>
    <ModelData Name="combDatabinned"/>
    <RenameMap>
      &13TeV_strong_npmap.xml;
    </RenameMap>
  </Channel>
  <Channel Name="high_mass_diphoton_8TeV">
    <File Name="#INPUT8TeV#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="xs,dummy,mG,GkM"/>
    <ModelData Name="combDatabinned"/>
    <RenameMap>
      &8TeV_strong_npmap.xml;
    </RenameMap>
  </Channel>
</Combination>
