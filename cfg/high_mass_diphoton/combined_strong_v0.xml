<!DOCTYPE Combination  SYSTEM 'Combination.dtd'>
<!-- Range 1: from 700 GeV (included) to 1000 GeV (not included). lvqq+llqq  -->
<Combination>
  <Channel Name="combined" IsCombined="true" Mass="750">
    <File Name="./combined.root"/>
    <Workspace Name="combWS"/>
    <ModelConfig  Name="ModelConfig"/>
    <ModelData Name="combData"/>
    <ModelPOI Name="xs[0~100],dxs[-100~100],mG[760],GkM[0.2]"/>
  </Channel>
  <Channel Name="high_mass_diphoton_13TeV">
    <File Name="workspace/high_mass_diphoton/2015_Graviton_histfactory_EKEI_v5_dxs.root"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="xs,dxs,mG,GkM"/>
    <ModelData Name="combData"/>
    <RenameMap>
<Syst OldName =     "ATLAS_lumi_2015"       NewName =       "ATLAS_lumi_2015" />
<Syst OldName =       "ATLAS_PH_EFF"       NewName =       "ATLAS_PH_EFF" />
<Syst OldName =       "ATLAS_mRes"       NewName =       "ATLAS_mRes" />
<Syst OldName =       "ATLAS_PH_ISO"       NewName =       "ATLAS_PH_ISO" />
<Syst OldName =       "alpha_bkg_irreducible"       NewName =       "ATLAS_bkg_irreducible" />
<Syst OldName =       "alpha_bkg_reducible"       NewName =       "ATLAS_bkg_reducible" />
<Syst OldName =       "alpha_bkg_purity"       NewName =       "ATLAS_bkg_purity" />
<Syst OldName =       "alpha_bkg_isolation"       NewName =       "ATLAS_bkg_isolation" />
<Syst OldName =                    "channellist"       NewName =                    "Cat_13TeV" />
    </RenameMap>
  </Channel>
  <Channel Name="high_mass_diphoton_8TeV">
    <File Name="workspace/high_mass_diphoton/2012_Graviton_histfactory_EKEI_v1.root"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="xs,dummy,mG,GkM"/>
    <ModelData Name="combData"/>
    <RenameMap>
<Syst OldName =     "ATLAS_lumi_2012"       NewName =       "ATLAS_lumi_2012" />
<Syst OldName =       "ATLAS_PH_EFF"       NewName =       "ATLAS_PH_EFF" />
<Syst OldName =       "ATLAS_mRes"       NewName =       "ATLAS_mRes" />
<Syst OldName =       "ATLAS_PH_ISO"       NewName =       "ATLAS_PH_ISO" />
<Syst OldName =       "alpha_bkg_irreducible"       NewName =       "ATLAS_bkg_irreducible" />
<Syst OldName =       "alpha_bkg_reducible"       NewName =       "ATLAS_bkg_reducible" />
<Syst OldName =       "alpha_bkg_purity"       NewName =       "ATLAS_bkg_purity" />
<Syst OldName =       "alpha_bkg_isolation"       NewName =       "ATLAS_bkg_isolation" />
<Syst OldName =                    "channellist"       NewName =                    "Cat_8TeV" />
    </RenameMap>
  </Channel>
</Combination>
