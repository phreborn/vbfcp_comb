<!DOCTYPE Organization  SYSTEM 'Organization.dtd'>
<Organization InFile="#INPUT#" OutFile="#OUTPUT#"
	     ModelName="deltamu"
	     POINames="xs, dxs, #OTHERPOI#">
  <Item Name="xs[1,0,100]"/>
  <Item Name="dxs[1,0,100]"/>
  <Item Name="expr::xs_modified('(@0*@2+@1)',xs,dxs,scale_factor[#SCALE#])"/>
  <!-- PDF -->
  <Item Name="EDIT::NEWPDF(OLDPDF,
	      xs=xs_modified,
	      )"/>
</Organization>
