<!DOCTYPE Orgnization  SYSTEM 'Orgnization.dtd'>
<Orgnization InFile="workspace/lhchcg/myrep/atlas/handler/atlas_h4l_toy_handler.root" OutFile="workspace/lhchcg/myrep/atlas/parambrlumi/atlas_h4l_toy_parambrlumi.root"
	     ModelName="param_BR"
	     POINames="MH,mu_zz">
  <Item Name="zero[0]"/>
  <Item Name="one[1]"/>

  <!-- Introducing kappa values -->
  <!-- H->zz -->
  <Item Name="kappa_value_BR_param_alphaS_zz[1.012]"/>
  <Item Name="kappa_value_BR_param_mB_zz[0.981]"/>
  <Item Name="kappa_value_BR_HiggsDecayWidthTHU_hqq_zz[0.988]"/>
  <Item Name="kappa_value_BR_HiggsDecayWidthTHU_hvv_zz[1.004]"/>

  <Item Name="expr::log_kappa_value_LHC_LUMI_2011('sqrt(log(1+@0*@0))', uncert_LUMI_2011_correlate[0.005])"/>
  <Item Name="expr::log_kappa_value_LHC_LUMI_2012('sqrt(log(1+@0*@0))', uncert_LUMI_2012_correlate[0.011])"/>
  <Item Name="expr::log_kappa_value_ATLAS_LUMI_uncor_2011('sqrt(log(1+@0*@0))', uncert_LUMI_2011_uncorrelate[0.017])"/>
  <Item Name="expr::log_kappa_value_ATLAS_LUMI_uncor_2012('sqrt(log(1+@0*@0))', uncert_LUMI_2012_uncorrelate[0.025])"/>

  <!-- Introducing nuisance parameter names -->
  <Item Name="BR_param_alphaS[0,-5,5]"/>
  <Item Name="BR_param_mB[0,-5,5]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hqq[0,-5,5]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hvv[0,-5,5]"/>

  <Item Name="LHC_LUMI_2011[0,-5,5]"/>
  <Item Name="LHC_LUMI_2012[0,-5,5]"/>
  <Item Name="ATLAS_LUMI_uncor_2011[0,-5,5]"/>
  <Item Name="ATLAS_LUMI_uncor_2012[0,-5,5]"/>

  <!-- Introducing global observables -->
  <Item Name="BR_param_alphaS_In[0]"/>
  <Item Name="BR_param_mB_In[0]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hqq_In[0]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hvv_In[0]"/>

  <Item Name="LHC_LUMI_2011_In[0]"/>
  <Item Name="LHC_LUMI_2012_In[0]"/>
  <Item Name="ATLAS_LUMI_uncor_2011_In[0]"/>
  <Item Name="ATLAS_LUMI_uncor_2012_In[0]"/>

  <!-- Introducing constraint terms -->
  <Item Name="RooGaussian::BR_param_alphaS_Pdf(BR_param_alphaS,BR_param_alphaS_In,1)"/>
  <Item Name="RooGaussian::BR_param_mB_Pdf(BR_param_mB,BR_param_mB_In,1)"/>
  <Item Name="RooGaussian::BR_HiggsDecayWidthTHU_hqq_Pdf(BR_HiggsDecayWidthTHU_hqq,BR_HiggsDecayWidthTHU_hqq_In,1)"/>
  <Item Name="RooGaussian::BR_HiggsDecayWidthTHU_hvv_Pdf(BR_HiggsDecayWidthTHU_hvv,BR_HiggsDecayWidthTHU_hvv_In,1)"/>

  <Item Name="RooGaussian::LHC_LUMI_2011_Pdf(LHC_LUMI_2011,LHC_LUMI_2011_In,1)"/>
  <Item Name="RooGaussian::LHC_LUMI_2012_Pdf(LHC_LUMI_2012,LHC_LUMI_2012_In,1)"/>
  <Item Name="RooGaussian::ATLAS_LUMI_uncor_2011_Pdf(ATLAS_LUMI_uncor_2011,ATLAS_LUMI_uncor_2011_In,1)"/>
  <Item Name="RooGaussian::ATLAS_LUMI_uncor_2012_Pdf(ATLAS_LUMI_uncor_2012,ATLAS_LUMI_uncor_2012_In,1)"/>

  <!-- Introducing response terms -->
  <!-- H->zz -->
  <Item Name="prod::expected_BR_param_alphaS_zz(RooExponential::expTerm_BR_param_alphaS_zz(BR_param_alphaS, expr::log_kappa_value_BR_param_alphaS_zz('log(@0)', kappa_value_BR_param_alphaS_zz)),nominal_BR_param_alphaS_zz[1])"/>
  <Item Name="prod::expected_BR_param_mB_zz(RooExponential::expTerm_BR_param_mB_zz(BR_param_mB, expr::log_kappa_value_BR_param_mB_zz('log(@0)', kappa_value_BR_param_mB_zz)),nominal_BR_param_mB_zz[1])"/>
  <Item Name="prod::expected_BR_HiggsDecayWidthTHU_hqq_zz(RooExponential::expTerm_BR_HiggsDecayWidthTHU_hqq_zz(BR_HiggsDecayWidthTHU_hqq, expr::log_kappa_value_BR_HiggsDecayWidthTHU_hqq_zz('log(@0)', kappa_value_BR_HiggsDecayWidthTHU_hqq_zz)),nominal_BR_HiggsDecayWidthTHU_hqq_zz[1])"/>
  <Item Name="prod::expected_BR_HiggsDecayWidthTHU_hvv_zz(RooExponential::expTerm_BR_HiggsDecayWidthTHU_hvv_zz(BR_HiggsDecayWidthTHU_hvv, expr::log_kappa_value_BR_HiggsDecayWidthTHU_hvv_zz('log(@0)', kappa_value_BR_HiggsDecayWidthTHU_hvv_zz)),nominal_BR_HiggsDecayWidthTHU_hvv_zz[1])"/>

  <Item Name="prod::expected_LHC_LUMI_2011(RooExponential::expTerm_LHC_LUMI_2011(LHC_LUMI_2011, log_kappa_value_LHC_LUMI_2011),nominal_LHC_LUMI_2011[1])"/>
  <Item Name="prod::expected_LHC_LUMI_2012(RooExponential::expTerm_LHC_LUMI_2012(LHC_LUMI_2012, log_kappa_value_LHC_LUMI_2012),nominal_LHC_LUMI_2012[1])"/>
  <Item Name="prod::expected_ATLAS_LUMI_uncor_2011(RooExponential::expTerm_ATLAS_LUMI_uncor_2011(ATLAS_LUMI_uncor_2011, log_kappa_value_ATLAS_LUMI_uncor_2011),nominal_ATLAS_LUMI_uncor_2011[1])"/>
  <Item Name="prod::expected_ATLAS_LUMI_uncor_2012(RooExponential::expTerm_ATLAS_LUMI_uncor_2012(ATLAS_LUMI_uncor_2012, log_kappa_value_ATLAS_LUMI_uncor_2012),nominal_ATLAS_LUMI_uncor_2012[1])"/>

  <Item Name="prod:expected_BR_zz_new(expected_BR_param_alphaS_zz,expected_BR_param_mB_zz,expected_BR_HiggsDecayWidthTHU_hqq_zz,expected_BR_HiggsDecayWidthTHU_hvv_zz, expected_LHC_LUMI_2011, expected_LHC_LUMI_2012, expected_ATLAS_LUMI_uncor_2011, expected_ATLAS_LUMI_uncor_2012, mu_BR_ZZ[1])"/>

  <Item Name= "EDIT::NEWPDF(OLDPDF,
       	       alpha_ATLAS_BR_VV=zero,
	       alpha_ATLAS_LUMI_2011=zero,
	       alpha_ATLAS_LUMI_2012=zero,
	       alpha_ATLAS_BR_VVConstraint=one,
	       alpha_ATLAS_LUMI_2011Constraint=one,
	       alpha_ATLAS_LUMI_2012Constraint=one,
	       mu_BR_ZZ_temp=expected_BR_zz_new,
       	       )" />
</Orgnization>
