<!DOCTYPE Orgnization  SYSTEM 'Orgnization.dtd'>
<Orgnization InFile="workspace/lhchcg/myrep/atlas/handler/atlas_h4l_asimov_handler.root" OutFile="workspace/lhchcg/myrep/atlas/parambr/atlas_h4l_asimov_parambr.root"
	     ModelName="param_BR"
	     POINames="MH,mu_zz">
  <Item Name="zero[0]"/>
  <Item Name="one[1]"/>

  <!-- Introducing kappa values -->
  <!-- H->zz -->
  <Item Name="kappa_value_BR_param_alphaS_zz[1.012]"/>
  <Item Name="kappa_value_BR_param_mB_zz[0.981]"/>
  <Item Name="kappa_value_BR_HiggsDecayWidthTHU_hqq_zz[0.988]"/>
  <Item Name="kappa_value_BR_HiggsDecayWidthTHU_hvv_zz[1.004]"/>

  <!-- Introducing nuisance parameter names -->
  <Item Name="BR_param_alphaS[0,-5,5]"/>
  <Item Name="BR_param_mB[0,-5,5]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hqq[0,-5,5]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hvv[0,-5,5]"/>

  <!-- Introducing global observables -->
  <Item Name="BR_param_alphaS_In[0]"/>
  <Item Name="BR_param_mB_In[0]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hqq_In[0]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hvv_In[0]"/>

  <!-- Introducing constraint terms -->
  <Item Name="RooGaussian::BR_param_alphaS_Pdf(BR_param_alphaS,BR_param_alphaS_In,1)"/>
  <Item Name="RooGaussian::BR_param_mB_Pdf(BR_param_mB,BR_param_mB_In,1)"/>
  <Item Name="RooGaussian::BR_HiggsDecayWidthTHU_hqq_Pdf(BR_HiggsDecayWidthTHU_hqq,BR_HiggsDecayWidthTHU_hqq_In,1)"/>
  <Item Name="RooGaussian::BR_HiggsDecayWidthTHU_hvv_Pdf(BR_HiggsDecayWidthTHU_hvv,BR_HiggsDecayWidthTHU_hvv_In,1)"/>

  <!-- Introducing response terms -->
  <!-- H->zz -->
  <Item Name="prod::expected_BR_param_alphaS_zz(RooExponential::expTerm_BR_param_alphaS_zz(BR_param_alphaS, expr::log_kappa_value_BR_param_alphaS_zz('sqrt(log(@0))', kappa_value_BR_param_alphaS_zz)),nominal_BR_param_alphaS_zz[1])"/>
  <Item Name="prod::expected_BR_param_mB_zz(RooExponential::expTerm_BR_param_mB_zz(BR_param_mB, expr::log_kappa_value_BR_param_mB_zz('sqrt(log(@0))', kappa_value_BR_param_mB_zz)),nominal_BR_param_mB_zz[1])"/>
  <Item Name="prod::expected_BR_HiggsDecayWidthTHU_hqq_zz(RooExponential::expTerm_BR_HiggsDecayWidthTHU_hqq_zz(BR_HiggsDecayWidthTHU_hqq, expr::log_kappa_value_BR_HiggsDecayWidthTHU_hqq_zz('sqrt(log(@0))', kappa_value_BR_HiggsDecayWidthTHU_hqq_zz)),nominal_BR_HiggsDecayWidthTHU_hqq_zz[1])"/>
  <Item Name="prod::expected_BR_HiggsDecayWidthTHU_hvv_zz(RooExponential::expTerm_BR_HiggsDecayWidthTHU_hvv_zz(BR_HiggsDecayWidthTHU_hvv, expr::log_kappa_value_BR_HiggsDecayWidthTHU_hvv_zz('sqrt(log(@0))', kappa_value_BR_HiggsDecayWidthTHU_hvv_zz)),nominal_BR_HiggsDecayWidthTHU_hvv_zz[1])"/>
  <Item Name="prod:expected_BR_zz_new(expected_BR_param_alphaS_zz,expected_BR_param_mB_zz,expected_BR_HiggsDecayWidthTHU_hqq_zz,expected_BR_HiggsDecayWidthTHU_hvv_zz,mu_BR_zz[1])"/>

  <Item Name= "EDIT::NEWPDF(OLDPDF,
       	       alpha_ATLAS_BR_VV=zero,
	       alpha_ATLAS_BR_VVConstraint=one,
	       mu_BR_zz_temp=expected_BR_zz_new,
       	       )" />
</Orgnization>
