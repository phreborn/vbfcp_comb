<!DOCTYPE Orgnization  SYSTEM 'Orgnization.dtd'>
<Orgnization InFile="#INPUT#" OutFile="#OUTPUT#"
	     ModelName="param_BR"
	     POINames="MH,RF_gg,RV_gg">
  <Item Name="zero[0]"/>
  <Item Name="one[1]"/>

  <!-- Introducing kappa values -->
  <!-- H->gamgam -->
  <Item Name="kappa_value_BR_param_alphaS_gamgam[1.012]"/>
  <Item Name="kappa_value_BR_param_mB_gamgam[0.981]"/>
  <Item Name="kappa_value_BR_HiggsDecayWidthTHU_hqq_gamgam[0.988]"/>
  <Item Name="kappa_value_BR_HiggsDecayWidthTHU_hgg_gamgam[1.010]"/>

  <!-- Introducing nuisance parameter names -->
  <Item Name="BR_param_alphaS[0,-5,5]"/>
  <Item Name="BR_param_mB[0,-5,5]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hqq[0,-5,5]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hgg[0,-5,5]"/>

  <!-- Introducing global observables -->
  <Item Name="BR_param_alphaS_In[0]"/>
  <Item Name="BR_param_mB_In[0]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hqq_In[0]"/>
  <Item Name="BR_HiggsDecayWidthTHU_hgg_In[0]"/>

  <!-- Introducing constraint terms -->
  <Item Name="RooGaussian::BR_param_alphaS_Pdf(BR_param_alphaS,BR_param_alphaS_In,1)"/>
  <Item Name="RooGaussian::BR_param_mB_Pdf(BR_param_mB,BR_param_mB_In,1)"/>
  <Item Name="RooGaussian::BR_HiggsDecayWidthTHU_hqq_Pdf(BR_HiggsDecayWidthTHU_hqq,BR_HiggsDecayWidthTHU_hqq_In,1)"/>
  <Item Name="RooGaussian::BR_HiggsDecayWidthTHU_hgg_Pdf(BR_HiggsDecayWidthTHU_hgg,BR_HiggsDecayWidthTHU_hgg_In,1)"/>

  <!-- Introducing response terms -->
  <!-- H->gamgam -->
  <Item Name="prod::expected_BR_param_alphaS_gamgam(RooExponential::expTerm_BR_param_alphaS_gamgam(BR_param_alphaS, expr::log_kappa_value_BR_param_alphaS_gamgam('sqrt(log(@0))', kappa_value_BR_param_alphaS_gamgam)),nominal_BR_param_alphaS_gamgam[1])"/>
  <Item Name="prod::expected_BR_param_mB_gamgam(RooExponential::expTerm_BR_param_mB_gamgam(BR_param_mB, expr::log_kappa_value_BR_param_mB_gamgam('sqrt(log(@0))', kappa_value_BR_param_mB_gamgam)),nominal_BR_param_mB_gamgam[1])"/>
  <Item Name="prod::expected_BR_HiggsDecayWidthTHU_hqq_gamgam(RooExponential::expTerm_BR_HiggsDecayWidthTHU_hqq_gamgam(BR_HiggsDecayWidthTHU_hqq, expr::log_kappa_value_BR_HiggsDecayWidthTHU_hqq_gamgam('sqrt(log(@0))', kappa_value_BR_HiggsDecayWidthTHU_hqq_gamgam)),nominal_BR_HiggsDecayWidthTHU_hqq_gamgam[1])"/>
  <Item Name="prod::expected_BR_HiggsDecayWidthTHU_hgg_gamgam(RooExponential::expTerm_BR_HiggsDecayWidthTHU_hgg_gamgam(BR_HiggsDecayWidthTHU_hgg, expr::log_kappa_value_BR_HiggsDecayWidthTHU_hgg_gamgam('sqrt(log(@0))', kappa_value_BR_HiggsDecayWidthTHU_hgg_gamgam)),nominal_BR_HiggsDecayWidthTHU_hgg_gamgam[1])"/>
  <Item Name="prod:expected_BR_gamgam_new(expected_BR_param_alphaS_gamgam,expected_BR_param_mB_gamgam,expected_BR_HiggsDecayWidthTHU_hqq_gamgam,expected_BR_HiggsDecayWidthTHU_hgg_gamgam,mu_BR_gamgam[1])"/>

  <Item Name= "EDIT::NEWPDF(OLDPDF,
       	       CMS_hgg_BR=zero,
	       CMS_hgg_BR_Pdf=one,
	       mu_BR_gamgam_temp=expected_BR_gamgam_new,
       	       )" />
</Orgnization>
