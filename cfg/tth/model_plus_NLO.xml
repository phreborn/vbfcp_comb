  <!-- VBF->H, 7TeV -->
  <Item Name="expr::C2_VBFH_7TeV('((@0*@0)*@2  + (@1*@1)*@3)', CZ, CW, CZFracVBF_7TeV[0.2548],CWFracVBF_7TeV[0.7452])"/>
