	      mu_ggH_8TeV=C2_GlGlH_8TeV,
	      mu_VBF=one,
	      mu_VBF_7TeV=C2_VBFH_7TeV,
	      mu_VBF_8TeV=C2_VBFH_7TeV,
	      mu_WH=CW2,
	      mu_ZH=CZ2,
	      mu_ttH=Ctop2,
	      mu_bbH=Cb2,
	      mu_tH=one,
