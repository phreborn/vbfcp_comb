<!DOCTYPE Combination  SYSTEM 'Combination.dtd'>
<Combination>
  <Channel Name="combined" IsCombined="true" Mass="2000">
    <File Name="./combined.root"/>
    <Workspace Name="combWS"/>
    <ModelConfig  Name="ModelConfig"/>
    <ModelData Name="combData"/>
    <ModelPOI Name="mu[0~100]"/>
  </Channel>
  <Channel Name="qqqq">
    <File Name="#INPUTqqqq#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="mu"/>
    <ModelData Name="combData"/>
    <RenameMap>
      &qqqq_npmap.xml;
    </RenameMap>
  </Channel>
  <Channel Name="vvqq">
    <File Name="#INPUTvvqq#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="SigXsecOverSM"/>
    <ModelData Name="combData"/>
