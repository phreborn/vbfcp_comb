      &vvqq_npmap.xml;
    </RenameMap>
  </Channel>
  <Channel Name="llqq">
    <File Name="#INPUTllqq#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="mu"/>
    <ModelData Name="combData"/>
    <RenameMap>
      &llqq_npmap.xml;
    </RenameMap>
  </Channel>
  <Channel Name="lvqq">
    <File Name="#INPUTlvqq#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="mu"/>
    <ModelData Name="combData"/>
    <RenameMap>
      &lvqq_npmap.xml;
    </RenameMap>
  </Channel>
  <Channel Name="vh">
    <File Name="#INPUTvh#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="mu"/>
    <ModelData Name="combData"/>
    <RenameMap>
      &vh_npmap.xml;
    </RenameMap>
  </Channel>
</Combination>
