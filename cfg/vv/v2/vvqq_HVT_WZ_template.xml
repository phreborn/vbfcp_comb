      &vvqq_npmap.xml;
    </RenameMap>
  </Channel>
    <Channel Name="llqq_binned">
    <File Name="#INPUTllqq#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="mu"/>
    <ModelData Name="combData"/>
    <RenameMap>
      &llqq_npmap.xml;
    </RenameMap>
  </Channel>
  <Channel Name="lvqq_binned">
    <File Name="#INPUTlvqq#"/>
    <Workspace Name="combWS"/>
    <ModelConfig Name="ModelConfig"/>
    <ModelPOI Name="mu"/>
    <ModelData Name="combData"/>
